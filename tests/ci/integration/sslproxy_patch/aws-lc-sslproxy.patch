@@ -53,6 +53,10 @@
 #include <openssl/x509.h>
 #include <openssl/x509v3.h>
 #include <openssl/ocsp.h>
+#ifdef OPENSSL_IS_AWSLC
+#include <openssl/bio.h>
+#include <openssl/pem.h>
+#endif
 //#if OPENSSL_VERSION_NUMBER >= 0x30000000L && !defined(LIBRESSL_VERSION_NUMBER)
 //#include <openssl/core_names.h> // OSSL_PKEY_PARAM_RSA_E
 //#endif /* OPENSSL_VERSION_NUMBER >= 0x30000000L */
@@ -1295,6 +1299,26 @@ ssl_x509chain_use(SSL_CTX *sslctx, X509 *crt, STACK_OF(X509) *chain)
 X509 *
 ssl_x509_load(const char *filename)
 {
+#ifdef OPENSSL_IS_AWSLC
+	BIO *in = NULL;
+	X509 *crt = NULL;
+	int rv;
+
+	if (ssl_init() == -1)
+		return NULL;
+
+	in = BIO_new(BIO_s_file());
+	if (in == NULL)
+		goto leave1;
+	rv = BIO_read_filename(in, filename);
+	if (rv <= 0)
+		goto leave2;
+	crt = PEM_read_bio_X509(in, NULL, NULL, NULL);
+leave2:
+	BIO_free(in);
+leave1:
+	return crt;
+#else
 	X509 *crt = NULL;
 	SSL_CTX *tmpctx;
 	SSL *tmpssl;
@@ -1320,6 +1344,7 @@ leave2:
 	SSL_CTX_free(tmpctx);
 leave1:
 	return crt;
+#endif
 }
 
